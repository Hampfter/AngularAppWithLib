{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "my-lib": {
      "projectType": "library",
      "root": "projects/my-lib",
      "sourceRoot": "projects/my-lib/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/my-lib/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/my-lib/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/my-lib/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/my-lib/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "my-application": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "root": "projects/my-application",
      "sourceRoot": "projects/my-application/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/my-application",
            "index": "projects/my-application/src/index.html",
            "browser": "projects/my-application/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/my-application/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/my-application/src/favicon.ico",
              "projects/my-application/src/assets"
            ],
            "styles": [
              "projects/my-application/src/styles.less"
            ],
            "scripts": [],
            "server": "projects/my-application/src/main.server.ts",
            "prerender": true,
            "ssr": {
              "entry": "projects/my-application/server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "my-application:build:production"
            },
            "development": {
              "buildTarget": "my-application:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "my-application:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/my-application/tsconfig.spec.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/my-application/src/favicon.ico",
              "projects/my-application/src/assets"
            ],
            "styles": [
              "projects/my-application/src/styles.less"
            ],
            "scripts": []
          }
        }
      }
    },
    "app2": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "less"
        }
      },
      "root": "projects/app2",
      "sourceRoot": "projects/app2/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/app2",
            "index": "projects/app2/src/index.html",
            "browser": "projects/app2/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/app2/tsconfig.app.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/app2/src/favicon.ico",
              "projects/app2/src/assets"
            ],
            "styles": [
              "projects/app2/src/styles.less"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app2:build:production"
            },
            "development": {
              "buildTarget": "app2:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "app2:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/app2/tsconfig.spec.json",
            "inlineStyleLanguage": "less",
            "assets": [
              "projects/app2/src/favicon.ico",
              "projects/app2/src/assets"
            ],
            "styles": [
              "projects/app2/src/styles.less"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
